{
  "version": 2,
  "name": "stremio-cdn-videos",
  "alias": "stremio-cdn-videos",
  "builds": [
    { "src": "static/**/*", "use": "@now/static" },
    { "src": "**/*.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "serverless.js" }
  ]
}
